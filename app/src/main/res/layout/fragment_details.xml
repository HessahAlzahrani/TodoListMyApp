<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailsFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textViewdate2_id"
            android:layout_width="353dp"
            android:layout_height="74dp"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/button"
            app:layout_constraintEnd_toEndOf="@+id/textViewdate1_id"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/textViewdate1_id"
            app:layout_constraintTop_toBottomOf="@+id/textViewdate1_id" />

        <EditText
            android:id="@+id/textViewdate1_id"
            android:layout_width="350dp"
            android:layout_height="72dp"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/textViewdate2_id"
            app:layout_constraintEnd_toEndOf="@+id/textViewdescription_id"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/textViewdescription_id"
            app:layout_constraintTop_toBottomOf="@+id/textViewdescription_id" />

        <EditText
            android:id="@+id/textViewdescription_id"
            android:layout_width="351dp"
            android:layout_height="73dp"
            app:layout_constraintBottom_toTopOf="@+id/textViewdate1_id"
            app:layout_constraintEnd_toEndOf="@+id/textViewtitel_id"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/textViewtitel_id"
            app:layout_constraintTop_toBottomOf="@+id/textViewtitel_id" />

        <EditText
            android:id="@+id/textViewtitel_id"
            android:layout_width="370dp"
            android:layout_height="74dp"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/textViewdescription_id"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="21dp" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/textViewdate2_id"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/textViewdate2_id"
            app:layout_constraintTop_toBottomOf="@+id/textViewdate2_id" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>